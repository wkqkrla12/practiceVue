<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper   PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.pratice.web.repository.BoardRepository">


    <select id="findAll" resultType="Board">

        select *
        from board;

    </select>
    
    
    
    <insert id="saveBoard" parameterType="com.pratice.web.entity.Board">
        INSERT INTO board (no, hit, name, regdate, subject)
        VALUES (#{no}, #{hit}, #{name}, #{regdate}, #{subject})
    </insert>



</mapper>